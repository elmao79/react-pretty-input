{
  "name": "react-pretty-input",
  "version": "1.0.10",
  "description": "A pretty input form field for React projects.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --reporters",
    "test:unit": "mocha --reporter spec --require babel-core/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles 'src/**/*.spec.js'",
    "build": "webpack --mode production --config webpack.prod.js",
    "start-dev": "webpack-dev-server --mode development --open --config webpack.dev.js --hot --history-api-fallback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elmao79/react-pretty-input.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "input"
  ],
  "author": "OSCAR MARTIN <elmao79@gmail.com> (http://omartin.es)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elmao79/react-pretty-input/issues"
  },
  "homepage": "https://github.com/elmao79/react-pretty-input#readme",
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-asserttype": "^1.0.5",
    "compass-sass-mixins": "^0.12.7",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.9.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.3.3",
    "sass-loader": "^7.0.3",
    "sinon": "^6.1.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
